<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.shape.*?>

<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.myjavafxapp.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <Circle fx:id="circle" fill="blue" radius="100.0" stroke="BLACK" strokeType="INSIDE" />

    <Label fx:id="welcomeText" />
    <Button onAction="#onHelloButtonClick" text="Press Me!" />
   <Button fx:id="btnDont" alignment="CENTER" mnemonicParsing="false" prefHeight="50.0" prefWidth="100.0" text="Don't Press Me"
           textAlignment="CENTER" textFill="blue" wrapText="true"
           onAction="#onDontButtonClick"/>
</VBox>
